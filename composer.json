{
    "name": "raphaelvoisin/laravel-ringover",
    "type": "library",
    "description": "Ringover Notifications Channel for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "RaphaÃ«l Voisin",
            "email": "raphael.voisin@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/notifications": "~5.5 || ~6.0 || ~7.0",
        "illuminate/support": "~5.5 || ~6.0 || ~7.0",
        "raphaelvoisin/ringover-api": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "RaphaelVoisin\\NotificationChannels\\Ringover\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RaphaelVoisin\\NotificationChannels\\Ringover\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Ringover\\RingoverServiceProvider"
            ]
        }
    }
}
